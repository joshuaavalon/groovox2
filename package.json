{
  "name": "@groovox/groovox",
  "version": "1.0.0",
  "description": "User scripts for Groovox",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "build": "concurrently npm:build:* -m 3",
    "build:database": "npm run --prefix packages/database build",
    "lint": "eslint --ext .ts packages",
    "test": "concurrently npm:build:* -m 3",
    "introspect": "npm run --prefix packages/database introspect",
    "generate:database": "npm run --prefix packages/database generate",
    "generate:graphql": "npm run --prefix packages/graphql generate"
  },
  "keywords": [],
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/groovox/groovox#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/groovox/groovox.git"
  },
  "bugs": {
    "url": "https://github.com/groovox/groovox/issues"
  },
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "devDependencies": {
    "@joshuaavalon/eslint-config-typescript": "^5.0.0",
    "@types/node": "^16.11.6",
    "concurrently": "^5.3.0",
    "eslint": "^8.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  }
}
